{
	"extends": "airbnb-base",
	"rules": {
		"func-names": 0,
		"linebreak-style": 0,
		"no-console": 0,
    "no-debugger": 0,
    "spaced-comment": 0,
    "comma-dangle": 0,
    "strict": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions":0,
    "prefer-arrow-callback":0,
    "object-shorthand":0,
    "semi":0,
    "quotes":0
	}
}
